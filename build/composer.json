{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Project definition for cogswell.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "unblibraries/cogswell.lib.unb.ca",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/malsup/cycle/archive/3.0.3-a.zip"
                },
                "name": "malsup/jquery.cycle",
                "require": {
                    "composer/installers": "~1.0"
                },
                "type": "drupal-library",
                "version": "3.0.3-a"
            },
            "type": "package"
        }
    ],
    "require": {
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "~1.6.1",
        "drupal-composer/drupal-scaffold": "~2.2.0",
        "drupal/config_update": "1.1",
        "drupal/console": "~1.0",
        "drupal/core": "8.3.1",
        "drupal/features": "3.1",
        "drupal/inline_entity_form": "1.0-alpha6",
        "drupal/views_slideshow": "4.4",
        "drupal/zircon": "1.0-rc1",
        "drush/drush": "~8.1",
        "webflo/drupal-finder": "^0.2.1",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "drupal/devel": "1.0-alpha1",
        "drupal/drupal-extension": "~3.0",
        "drupal/migrate_plus": "3.0-beta1",
        "drupal/migrate_source_csv": "2.0-beta1",
        "drupal/migrate_tools": "3.0-beta1",
        "drupal/migrate_upgrade": "3.0-rc1",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
